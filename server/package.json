{
  "name": "server",
  "version": "0.0.1",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "NODE_ENV=development ts-node ./src/main",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "start": "NODE_ENV=production node -r ./config.js ./dist/main",
    "test": "NODE_ENV=test vitest",
    "type-check": "tsc -b --noEmit",
    "kysely": "kysely init",
    "seed:make": "kysely seed make",
    "seed:run": "kysely seed run",
    "migrate:make": "kysely migrate make",
    "migrate:up": "kysely migrate up",
    "migrate:down": "kysely migrate down",
    "migrate:latest": "kysely migrate latest",
    "codegen": "kysely-codegen --out-file ./src/database/db.d.ts"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@langchain/core": "^1.1.15",
    "@langchain/langgraph": "^1.1.0",
    "@langchain/openai": "^1.2.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-jwt": "^8.5.1",
    "helmet": "^8.1.0",
    "jet-logger": "^2.2.3",
    "jet-validators": "^2.2.5",
    "jsonfile": "^6.2.0",
    "jsonwebtoken": "^9.0.3",
    "kysely": "^0.28.10",
    "langchain": "^1.2.10",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "pg": "^8.17.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@swc/core": "^1.15.8",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.23.2",
    "find": "^0.3.0",
    "fs-extra": "^11.3.3",
    "jiti": "^2.6.1",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.20.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  }
}
